<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="devopsml" nsURI="http://www.example.org/devopsml" nsPrefix="devopsml">
  <eClassifiers xsi:type="ecore:EClass" name="DevOpsML">
    <eStructuralFeatures xsi:type="ecore:EReference" name="modelingcapability" upperBound="-1"
        eType="#//ModelingCapability" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="language" upperBound="-1"
        eType="#//Language" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="tool" upperBound="-1" eType="#//Tool"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ModelingCapability"/>
  <eClassifiers xsi:type="ecore:EClass" name="Language">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="languagecharacteristic"
        eType="#//LanguageCharacteristic" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="languagescope" upperBound="-1"
        eType="#//LanguageScope" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Tool"/>
  <eClassifiers xsi:type="ecore:EEnum" name="DevOpsPhase">
    <eLiterals name="CODE"/>
    <eLiterals name="BUILD" value="1"/>
    <eLiterals name="TEST" value="2"/>
    <eLiterals name="RELEASE" value="3"/>
    <eLiterals name="DEPLOY" value="4"/>
    <eLiterals name="OPERATE" value="5"/>
    <eLiterals name="MONITOR" value="6"/>
    <eLiterals name="PLAN" value="7"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="LanguageCharacteristic">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="abstractSyntax" lowerBound="1"
        eType="#//AsKind" defaultValueLiteral=""/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="concreteSyntax" lowerBound="1"
        upperBound="-1" eType="#//CsKind"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="semantics" lowerBound="1"
        eType="#//SemanticsKind" defaultValueLiteral=""/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="serialization" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="typing" eType="#//TypingKind"
        defaultValueLiteral=""/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="realization" eType="#//RealizationKind"
        defaultValueLiteral=""/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="AsKind">
    <eLiterals name="MOF"/>
    <eLiterals name="XML Schema" value="1"/>
    <eLiterals name="UML" value="2"/>
    <eLiterals name="NONE" value="3"/>
    <eLiterals name="GRAMMAR" value="4"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="SemanticsKind">
    <eLiterals name="OPERATIONAL"/>
    <eLiterals name="TRANSALTIONAL" value="1"/>
    <eLiterals name="NONE" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="CsKind">
    <eLiterals name="TEXTUAL"/>
    <eLiterals name="GRAPHICAL" value="1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="TypingKind">
    <eLiterals name="LINGUISTIC"/>
    <eLiterals name="ONTOLOGICAL" value="1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="LanguageScope">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="pragmatics" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="phase" eType="#//DevOpsPhase"
        defaultValueLiteral=""/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="RealizationKind">
    <eLiterals name="INTERNAL_DSL"/>
    <eLiterals name="EXTERNAL_DSL" value="1"/>
  </eClassifiers>
</ecore:EPackage>
